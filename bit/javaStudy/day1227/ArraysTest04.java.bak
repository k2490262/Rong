import java.util.Arrays;

class Person {
	private String name;
	private int age;

	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}

	public boolean equals(Object ob) {
		boolean re = false;
		Person p = (Person)ob;
		
		if (name.equals(p.name) && age == p.age) {
			re = true;
		}
		return re;
	}
}

